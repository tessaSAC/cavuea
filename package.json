{
  "name": "cavuea",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "tessaSAC <tessaSAC@users.noreply.github.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "lint": "run-s lint:all:*",
    "lint:pre-push": "yarn lint:all:eslint --max-warnings 0 && yarn lint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:prettier": "prettier --write --loglevel warn \"**/*.{js,json,css,scss,vue,html,md}\"",
    "lint:all:eslint": "eslint --fix --ext .js,.vue .",
    "minify:images": "imagemin \"**/*.{png,jpeg,jpg,gif,svg}\""
  },
  "gitHooks": {
    "pre-commit": "cross-env PRE_COMMIT=true lint-staged",
    "pre-push": "yarn lint:pre-push"
  },
  "dependencies": {
    "vue": "^2.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^6.0.0",
    "babel-preset-env": "^1.5.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^0.9.0",
    "imagemin-lint-staged": "^0.4.0",
    "lint-staged": "^9.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "vue-loader": "^12.1.0",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "yorkie": "^2.0.0"
  }
}
